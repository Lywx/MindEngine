<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
  </configSections>
  <nlog>
    <!--https://github.com/nlog/NLog/wiki/Configuration-file-->
    <target name="Process" xsi:type="File" fileName="MindEngine.Process.log"/>
    <logger name="MindEngine.Core.Service.Process.MMProcess" minlevel="Debug" writeTo="Process" />
  </nlog>
</configuration>